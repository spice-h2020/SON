@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix ex: <https://raw.githubusercontent.com/spice-h2020/SON/main/test/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

ex:test1.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "What are the interests of a certain person?" ;
 	owlunit:hasSPARQLUnitTest '''PREFIX earmark: <http://www.essepuntato.it/2008/12/earmark#> PREFIX semiotics: <http://ontologydesignpatterns.org/cp/owl/semiotics.owl#> SELECT DISTINCT ?entity { ?pr earmark:begins ?beg . ?pr earmark:ends ?end . ?pr semiotics:denotes ?entity . }''' ;
	owlunit:hasInputData ex:datacq1.ttl ;
	owlunit:hasInputTestDataCategory owlunit:ToyDataset ;
	owlunit:hasExpectedResult "{  \"head\": {  \"vars\": [  \"interest\" ] } ,  \"results\": {  \"bindings\": [ {  \"interest\": {  \"type\":  \"uri\" ,  \"value\":  \"https://w3id.org/OWLunit/examples/Basketball\" } } ] } }";
	owlunit:testsOntology foaf: .
