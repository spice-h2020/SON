    <!-- https://w3id.org/spice/SON/ShaverEmotions/refersToPrototypeEmotion -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/ShaverEmotions/refersToPrototypeEmotion">
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/ShaverEmotions/EmotionWord"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/ShaverEmotions/PrototypeEmotion"/>
        <exuviae:bibRef xml:lang="en">Shaver P, Schwartz J, Kirson D, O&apos;Connor C. Emotion knowledge: further exploration of a prototype approach. J Pers Soc Psychol. 1987 Jun;52(6):1061-86. doi: 10.1037//0022-3514.52.6.1061. PMID: 3598857.</exuviae:bibRef>
        <rdfs:comment xml:lang="en">This property was introduced to link an emotion word to the Emotion Prototype that was chosen to identify a cluster in Shaver studies.</rdfs:comment>
        <rdfs:label xml:lang="en">refers to prototype emotion</rdfs:label>
    </owl:ObjectProperty>
    


