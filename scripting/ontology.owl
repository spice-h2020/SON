  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/RoleInTime">
    <rdfs:label xml:lang="en">Role in time</rdfs:label>
    <rdfs:comment xml:lang="en">A time-indexed situation in which an agent holds a role.</rdfs:comment>
