    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Ontology rdf:about="https://w3id.org/spice/SON/scripting">
    <owl:versionIRI rdf:resource="https://w3id.org/spice/SON/scripting/0.0.2"/>
    <dc:description rdf:resource="https://raw.githubusercontent.com/spice-h2020/SON/main/scripting/scripting.jpg"/>
    <owl:imports rdf:resource="http://purl.org/net/p-plan#"/>
    <terms:license rdf:resource="http://creativecommons.org/licenses/by/4.0/"/>
    <owl:priorVersion rdf:resource="https://w3id.org/spice/SON/scripting/0.0.1"/>
    <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl"/>
    <dc:title xml:lang="en">Scripting Ontology</dc:title>
    <terms:creator rdf:resource="http://www.wikidata.org/entity/Q97066487"/>
    <terms:creator rdf:resource="http://www.wikidata.org/entity/Q56457363"/>
    <owl:imports rdf:resource="http://purl.org/spar/pso/2014-02-01"/>
    <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl"/>
    <rdfs:comment xml:lang="en">The Scripting ontology is an ontology for describing the design and the execution of citizen curation activities, such as engagement activities with museum visitors, web applications for eliciting users' interpretations (e.g. via storytelling, question answering), social media interactions.</rdfs:comment>
    <dc:creator xml:lang="en">Marilena Daquino</dc:creator>
    <dc:creator xml:lang="en">Paul Mulholland</dc:creator>
    <terms:creator rdf:resource="http://www.wikidata.org/entity/Q96435625"/>
    <owl:imports rdf:resource="http://www.w3.org/ns/prov-o-20130430"/>
    <dc:creator xml:lang="en">Luigi Asprino</dc:creator>
  </owl:Ontology>
  <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal">
    <rdfs:label xml:lang="en">Goal</rdfs:label>
    <rdfs:comment xml:lang="en">The purpose of an entity, such as a script.</rdfs:comment>
  </owl:Class>
