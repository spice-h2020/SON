      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Suggesting"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/ManualSelectionOfUserGeneratedContents">
    <rdfs:label xml:lang="en">Manual selection of user-generated contents</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/SelectionOfUserGeneratedContents"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/QuestionnaireFilling">
    <rdfs:label xml:lang="en">Questionnaire filling</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Production"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/VideoProduction">
    <rdfs:label xml:lang="en">Video production</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/ProductionOfMultimediaContent"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/ns/prov#Role"/>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/ScriptExecution">
    <rdfs:label xml:lang="en">Script execution</rdfs:label>
    <rdfs:comment xml:lang="en">The execution of a number of sequential actions, as planned in a script.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#WorkflowExecution"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/SelectionOfMultimediaContents">
    <rdfs:label xml:lang="en">Selection of multimedia contents</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Selection"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/SuggestingToRegister">
    <rdfs:label xml:lang="en">Suggesting to register</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/SuggestingAnAction"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/DefinitionOfUserModel">
    <rdfs:label xml:lang="en">Definition of a User Model</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Processing"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Filtering">
    <rdfs:label xml:lang="en">Filtering</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Interaction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Role">
    <rdfs:label xml:lang="en">Role</rdfs:label>
    <rdfs:comment xml:lang="en">The role of an agent or a task in a script.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Role"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/ResultsPresentation">
    <rdfs:label xml:lang="en">Results presentation</rdfs:label>
    <rdfs:comment xml:lang="en">The stage of a script where results of prior activities are presented to stakeholders.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Stage"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/SendingDigitalContent">
    <rdfs:label xml:lang="en">Sending a digital content</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Rewarding"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Browsing">
    <rdfs:label xml:lang="en">Browsing</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Interaction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Task">
    <rdfs:label xml:lang="en">Task</rdfs:label>
    <rdfs:comment xml:lang="en">A task to be accomplished as specified in a script or a stage of the script.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://purl.org/net/p-plan#Step"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Manipulation">
    <rdfs:label xml:lang="en">Manipulation</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Interaction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/ResultsAnalysis">
    <rdfs:label xml:lang="en">Results analysis</rdfs:label>
    <rdfs:comment xml:lang="en">The stage in which results of the Running stage are analysed, e.g. review of users' answers and stories.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Stage"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#TimeInterval">
    <rdfs:label xml:lang="en">Time interval</rdfs:label>
    <rdfs:comment xml:lang="en">A representation of a time interval, characterised by a start and an end.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Running">
    <rdfs:label xml:lang="en">Running stage</rdfs:label>
    <rdfs:comment xml:lang="en">The stage in which the main activities designed in the script are executed. E.g. in an engagement activity: users create stories, share opinions, answer questions.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Stage"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/SelectionOfCuratorialContents">
    <rdfs:label xml:lang="en">Selection of curatorial contents</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Selection"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Action">
    <rdfs:label xml:lang="en">Action</rdfs:label>
    <rdfs:comment xml:lang="en">An action is the execution of a task specified in a script.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.w3.org/ns/prov#Activity"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action"/>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/net/p-plan#MultiStep"/>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Rewarding">
    <rdfs:label xml:lang="en">Rewarding</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Interaction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Agent">
    <rdfs:label xml:lang="en">Agent</rdfs:label>
    <rdfs:comment xml:lang="en">An entity involved in an action executing a task.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.w3.org/ns/prov#Agent"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Design">
    <rdfs:label xml:lang="en">Design</rdfs:label>
    <rdfs:comment xml:lang="en">In this task agents design contents, e.g. design a questionnaire for museum visitors.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/PostPublicationOnSocialMediaPlatform">
    <rdfs:label xml:lang="en">Post publication on a Social Media Platform</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/WebPublication"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/FreeTextAnswering">
    <rdfs:label xml:lang="en">Free-text answering</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/QuestionAnswering"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/WebPublication">
    <rdfs:label xml:lang="en">Web publication</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Presentation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Stage">
    <rdfs:label xml:lang="en">Stage</rdfs:label>
    <rdfs:comment xml:lang="en">The grouping of tasks to be executed in sequence, as planned in a script.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Workflow"/>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/p-plan#MultiStep"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/UserGeneratedContentPresentation">
    <rdfs:label xml:lang="en">Presentation of user-generated content</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Presentation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/AddEmoticon">
    <rdfs:label xml:lang="en">Add an emoticon</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/QuestionAnswering"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Collection">
    <rdfs:label xml:lang="en">Collection</rdfs:label>
    <rdfs:comment xml:lang="en">In this task agents group materials in a collection, e.g. a collection of contents to be shown to visitors. Collecting does not imply selecting contents.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/QuestionAnswering">
    <rdfs:label xml:lang="en">Question Answering</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Interaction"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://purl.org/net/p-plan#Variable"/>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/SelectionOfNickname">
    <rdfs:label xml:lang="en">Selection of a nickname</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/SelectionOfIdentifier"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event">
    <rdfs:label xml:lang="en">Event</rdfs:label>
    <rdfs:comment xml:lang="en">An event in which a script is executed, e.g. a workshop, an exhibition.</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/PictureProduction">
    <rdfs:label xml:lang="en">Picture production</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/ProductionOfMultimediaContent"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Presentation">
    <rdfs:label xml:lang="en">Presentation</rdfs:label>
    <rdfs:comment xml:lang="en">In this task agents present contents to an audience, e.g. publish a post on a social media platform, show an exhibit, present results of an activity.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/MultipleChoiceAnswering">
    <rdfs:label xml:lang="en">Multiple choice answering</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/QuestionAnswering"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/ProductionOfMultimediaContent">
    <rdfs:label xml:lang="en">Production of multimedia content</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Production"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Interaction">
    <rdfs:label xml:lang="en">Interaction</rdfs:label>
    <rdfs:comment xml:lang="en">In this task different agents interact with each other to create/obtain contents, e.g. answering questions to create stories, suggest actions to get rewards.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/PreliminaryAnalysis">
    <rdfs:label xml:lang="en">Preliminary analysis</rdfs:label>
    <rdfs:comment xml:lang="en">The stage of the script in which partial results of activities are analysed, e.g. to develop a User Model for recommendation purposes, and whose results are likely to be used in the Running stage.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/Stage"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/ActivityPreparation">
    <rdfs:label xml:lang="en">Activity preparation</rdfs:label>
    <rdfs:comment xml:lang="en">The stage in which all activities to be performed in a script are prepared (e.g. selecting materials, designing questionnaires, making videos).</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/Stage"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/SelectionOfArtefacts">
    <rdfs:label xml:lang="en">Selection of artefacts</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/SelectionOfCuratorialContents"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Variable">
    <rdfs:label xml:lang="en">Variable</rdfs:label>
    <rdfs:comment xml:lang="en">A variable entity used as input or generated as output of a task.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://purl.org/net/p-plan#Variable"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Selection">
    <rdfs:label xml:lang="en">Selection</rdfs:label>
    <rdfs:comment xml:lang="en">In this task agents select any type of content among a collection of contents available. E.g. select artefacts from a museum collection for an exhibition, select only most interesting user-generated contents, select your own avatar (among other users' avatars) in a web application.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/SelectionOfUserGeneratedContents">
    <rdfs:label xml:lang="en">Selection of user-generated contents</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/Selection"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Production">
    <rdfs:label xml:lang="en">Production</rdfs:label>
    <rdfs:comment xml:lang="en">In this task agents produce new contents, e.g. make a video, take a picture, fill in a questionnaire, write a story.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.w3.org/2002/07/owl#Class"/>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/ExemplarContentPresentation">
    <rdfs:label xml:lang="en">Presentation of exemplar content</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/Presentation"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/Suggesting">
    <rdfs:label xml:lang="en">Suggesting</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/scripting/Interaction"/>
  </owl:Class>
  <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#WorkflowExecution">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
