    <rdfs:comment xml:lang="en">A property relating a Task to any entity (object, device, person) that is relevant to the accomplishment of the task.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
    <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/hasOutputVariable">
    <rdfs:label xml:lang="en">has output variable</rdfs:label>
    <rdfs:comment xml:lang="en">The relation between a task and the expected output variable generated by the task. The specified output has to be considered as mandatory. Use hasConditionalOutput to specify optional outputs of the task.</rdfs:comment>
    <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Variable"/>
    <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
    <rdfs:subPropertyOf rdf:resource="http://purl.org/net/p-plan#hasOutputVar"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://www.w3.org/2002/07/owl#unionOf"/>
