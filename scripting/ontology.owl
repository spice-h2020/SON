     -->

    


    <!-- http://purl.org/net/p-plan#correspondsToVariable -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/p-plan#correspondsToVariable"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expands -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expands"/>
    


    <!-- http://www.w3.org/2002/07/owl#unionOf -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2002/07/owl#unionOf"/>
    


    <!-- https://w3id.org/spice/SON/scripting/belongsTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/belongsTo">
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Variable"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
        <rdfs:comment xml:lang="en">A property using punning to define a restriction on the type associated to a Variable for validating the result of an action in script execution.</rdfs:comment>
        <rdfs:label xml:lang="en">belongs to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/conditionallyFollows -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/conditionallyFollows">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <owl:inverseOf rdf:resource="https://w3id.org/spice/SON/scripting/conditionallyPrecedes"/>
        <rdfs:comment xml:lang="en">A property relating a Task that may follow a preceding task if some condition is satisfied. E.g. If the user inputs her/his email in a form, a welcome email is sent to him/her. The task &quot;Send the welcome email&quot; conditionally follows the task &quot;user inputs his/her email&quot;.</rdfs:comment>
        <rdfs:label xml:lang="en">conditionally follows</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/conditionallyPrecedes -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/conditionallyPrecedes">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdfs:comment xml:lang="en">A property relating a Task that may preceed a following task. E.g. A welcome email is sent to a user that either input his/her email in a form or whose email was already in the system. The task &quot;user inputs his/her email&quot; conditionally preceeds the task &quot;Send the welcome email&quot; .</rdfs:comment>
        <rdfs:label xml:lang="en">conditionally precedes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/correspondsTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/correspondsTo">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/p-plan#correspondsToVariable"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Variable"/>
        <rdfs:comment xml:lang="en">The relation between an input/output of an action and the variable defined in the task.</rdfs:comment>
        <rdfs:label xml:lang="en">corresponds to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/definesStage -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/definesStage">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#expands"/>
        <owl:inverseOf rdf:resource="https://w3id.org/spice/SON/scripting/isStageOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Script"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Stage"/>
        <rdfs:comment xml:lang="en">The relation between a script and the stages defined by the script.</rdfs:comment>
        <rdfs:label xml:lang="en">defines stage</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/definesTask -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/definesTask">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#definesTask"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
        <rdfs:comment xml:lang="en">The relation between a script or a stage and the task that is there defined.</rdfs:comment>
        <rdfs:label xml:lang="en">defines task</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/executedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/executedIn">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isExecutedIn"/>
        <owl:inverseOf rdf:resource="https://w3id.org/spice/SON/scripting/executesTask"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Action"/>
        <rdfs:comment xml:lang="en">The relation between a task (defined in a script) and its execution in an action.</rdfs:comment>
        <rdfs:label xml:lang="en">executed in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/executesTask -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/executesTask">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#executesTask"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Action"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
        <rdfs:comment xml:lang="en">The relation between the execution of a task (an action) and the task executed.</rdfs:comment>
        <rdfs:label xml:lang="en">executes task</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/follows -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/follows">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#directlyFollows"/>
        <owl:inverseOf rdf:resource="https://w3id.org/spice/SON/scripting/precedes"/>
        <rdfs:comment xml:lang="en">The relation between an entity and the entity that follows it in a sequence.</rdfs:comment>
        <rdfs:label xml:lang="en">follows</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/forAction -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/forAction">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity"/>
        <owl:inverseOf rdf:resource="https://w3id.org/spice/SON/scripting/hasRoleInTime"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/RoleInTime"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Action"/>
        <rdfs:comment xml:lang="en">The relation between a situation (a role in time) and the action to which it is relevant.</rdfs:comment>
        <rdfs:label xml:lang="en">for action</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/generated -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/generated">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#generated"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Action"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">The relation between an action and the entity generated by the action.</rdfs:comment>
        <rdfs:label xml:lang="en">generated</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/hasConditionalInput -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/hasConditionalInput">
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
        <rdfs:range rdf:resource="http://purl.org/net/p-plan#Variable"/>
        <rdfs:comment xml:lang="en">The relation between a task and the expected input variable used by the task. The specified input has to be considered as optional for the task. Use hasInput to specify mandatory inputs of the task.</rdfs:comment>
        <rdfs:label xml:lang="en">has conditional input</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/hasConditionalOutput -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/hasConditionalOutput">
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:range rdf:resource="http://purl.org/net/p-plan#Variable"/>
        <rdfs:comment xml:lang="en">The relation between a task and the expected output variable generated by the task. The specified output has to be considered as optional for the task. Use hasOutput to specify mandatory outputs of the task.</rdfs:comment>
        <rdfs:label xml:lang="en">has conditional output</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/hasInputVariable -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/hasInputVariable">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/p-plan#hasInputVar"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Variable"/>
        <rdfs:comment xml:lang="en">The relation between a task and the expected input variable used in the task (possibly to generate an output variable). The specified input has to be considered as mandatory. Use hasConditionalInput to specify optional inputs of  the task.</rdfs:comment>
        <rdfs:label xml:lang="en">has input variable</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/hasLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/hasLocation">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasLocation"/>
        <rdfs:comment xml:lang="en">The (physical or abstract) location of an entity.</rdfs:comment>
        <rdfs:label xml:lang="en">has location</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/hasOutputVariable -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/hasOutputVariable">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/p-plan#hasOutputVar"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Variable"/>
        <rdfs:comment xml:lang="en">The relation between a task and the expected output variable generated by the task. The specified output has to be considered as mandatory. Use hasConditionalOutput to specify optional outputs of the task.</rdfs:comment>
        <rdfs:label xml:lang="en">has output variable</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/hasPurpose -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/hasPurpose">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasComponent"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Goal"/>
        <rdfs:comment xml:lang="en">The purpose or goal of an event, an activity, or a plan (e.g. a script).</rdfs:comment>
        <rdfs:label xml:lang="en">has purpose</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/hasRoleInTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/hasRoleInTime">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#hasTimeIndexedSetting"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Action"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/RoleInTime"/>
        <rdfs:comment xml:lang="en">The relation between an action and a time-indexed situation.</rdfs:comment>
        <rdfs:label xml:lang="en">has role in time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/hasSetting -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/hasSetting">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasSetting"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Action"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
        <rdfs:comment xml:lang="en">The relation between an action and the setting for the action.</rdfs:comment>
        <rdfs:label xml:lang="en">has setting</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/hasTimeInterval -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/hasTimeInterval">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasTimeInterval"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#TimeInterval"/>
        <rdfs:comment xml:lang="en">The relation between an entity and the time interval in which it occurs.</rdfs:comment>
        <rdfs:label xml:lang="en">has time interval</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/includesAction -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/includesAction">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesAction"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/ScriptExecution"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Action"/>
        <rdfs:comment xml:lang="en">The relation between the execution of a script and the actions involved.</rdfs:comment>
        <rdfs:label xml:lang="en">includes action</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/involves -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/involves">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#associatedWith"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
        <rdfs:comment xml:lang="en">A property relating a Task to any entity (object, device, person) that is relevant to the accomplishment of the task.</rdfs:comment>
        <rdfs:label xml:lang="en">involves</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/isRoleInTimeOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/isRoleInTimeOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/RoleInTime"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Agent"/>
        <rdfs:comment xml:lang="en">The relation between a time-indexed situation and an agent.</rdfs:comment>
        <rdfs:label xml:lang="en">is role in time of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/isStageOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/isStageOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isExpandedIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Stage"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Script"/>
        <rdfs:comment xml:lang="en">The relation between a stage and the script that planned it.</rdfs:comment>
        <rdfs:label xml:lang="en">is stage of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/isTaskOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/isTaskOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isTaskOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Task"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Role"/>
        <rdfs:comment xml:lang="en">The relation between a task and its role in the script.</rdfs:comment>
        <rdfs:label xml:lang="en">is task of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/occursWithin -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/occursWithin">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isConstituentOf"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
        <rdfs:comment xml:lang="en">The hierarchical relation between (actions and) events.</rdfs:comment>
        <rdfs:label xml:lang="en">occurs within</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/precedes -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/precedes">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#directlyPrecedes"/>
        <rdfs:comment xml:lang="en">The relation between an entity and the entity that precedes it in a sequence.</rdfs:comment>
        <rdfs:label xml:lang="en">precedes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/satisfies -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/satisfies">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#satisfies"/>
        <rdfs:comment>The relation between a script execution and the script it relies on.</rdfs:comment>
        <rdfs:label>satisfies</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/used -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/used">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#used"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/Action"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="en">The relation between an action and the entity used during the action (possibly to generate a new entity).</rdfs:comment>
        <rdfs:label xml:lang="en">used</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/spice/SON/scripting/withRole -->

    <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/scripting/withRole">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity"/>
        <rdfs:domain rdf:resource="https://w3id.org/spice/SON/scripting/RoleInTime"/>
        <rdfs:range rdf:resource="https://w3id.org/spice/SON/scripting/Role"/>
        <rdfs:comment xml:lang="en">The relation between a time-indexed situation and the role held by an agent in the situation.</rdfs:comment>
        <rdfs:label xml:lang="en">with role</rdfs:label>
    </owl:ObjectProperty>
    


