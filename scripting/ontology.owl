      <owl:Class rdf:about="http://www.w3.org/ns/prov#Plan"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Workflow"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/scripting/SuggestingAnAction">
