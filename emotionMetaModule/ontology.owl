###  http://www.semanticweb.org/sten-doipanni/ontologies/2021/4/aoe.owl#EmotionPerspective
aoe:EmotionPerspective rdf:type owl:Class ;
                       owl:equivalentClass [ rdf:type owl:Class ;
                                             owl:unionOf ( [ rdf:type owl:Restriction ;
                                                             owl:onProperty dul:hasConstituent ;
                                                             owl:someValuesFrom aoe:EmoBehaviouralDescription
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty dul:hasConstituent ;
                                                             owl:someValuesFrom aoe:EmoCognitiveDescription
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty dul:hasConstituent ;
                                                             owl:someValuesFrom aoe:EmoLexicalDescription
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty dul:hasConstituent ;
                                                             owl:someValuesFrom aoe:EmoNeurophysiologicalDescription
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty dul:hasConstituent ;
                                                             owl:someValuesFrom aoe:EmoSocietalDescription
                                                           ]
                                                         )
                                           ] ;
                       rdfs:subClassOf aoe:EmotionDescription .


