    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotion -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotion">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#feltBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#appraises"/>
                                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#Emotion"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionOther -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionOther">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#feltBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#appraises"/>
                                <owl:someValuesFrom>
                                    <owl:Class>
                                        <owl:intersectionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                                            <owl:Class>
                                                <owl:unionOf rdf:parseType="Collection">
                                                    <owl:Restriction>
                                                        <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#blameworthy"/>
                                                        <owl:someValuesFrom rdf:resource="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                                                    </owl:Restriction>
                                                    <owl:Restriction>
                                                        <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#praiseworthy"/>
                                                        <owl:someValuesFrom rdf:resource="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                                                    </owl:Restriction>
                                                </owl:unionOf>
                                            </owl:Class>
                                            <owl:Restriction>
                                                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#performedBy"/>
                                                <owl:someValuesFrom rdf:resource="https://w3id.org/spice/SON/OCCEmotion#OtherAgent"/>
                                            </owl:Restriction>
                                        </owl:intersectionOf>
                                    </owl:Class>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotion"/>
        <owl:disjointWith rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionSelf"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionSelf -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionSelf">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#feltBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#appraises"/>
                                <owl:someValuesFrom>
                                    <owl:Class>
                                        <owl:intersectionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                                            <owl:Class>
                                                <owl:unionOf rdf:parseType="Collection">
                                                    <owl:Restriction>
                                                        <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#blameworthy"/>
                                                        <owl:someValuesFrom rdf:resource="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                                                    </owl:Restriction>
                                                    <owl:Restriction>
                                                        <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#praiseworthy"/>
                                                        <owl:someValuesFrom rdf:resource="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                                                    </owl:Restriction>
                                                </owl:unionOf>
                                            </owl:Class>
                                            <owl:Restriction>
                                                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#performedBy"/>
                                                <owl:someValuesFrom rdf:resource="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                                            </owl:Restriction>
                                        </owl:intersectionOf>
                                    </owl:Class>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotion"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#Admiration -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Admiration">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#feltBy"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#appraises"/>
                                <owl:someValuesFrom>
                                    <owl:Class>
                                        <owl:intersectionOf rdf:parseType="Collection">
                                            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                                            <owl:Restriction>
                                                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#performedBy"/>
                                                <owl:someValuesFrom rdf:resource="https://w3id.org/spice/SON/OCCEmotion#OtherAgent"/>
                                            </owl:Restriction>
                                            <owl:Restriction>
                                                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#praiseworthy"/>
                                                <owl:someValuesFrom rdf:resource="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                                            </owl:Restriction>
                                        </owl:intersectionOf>
                                    </owl:Class>
                                </owl:someValuesFrom>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionOther"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#PositiveOutcomeEmotion"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#Anger -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Anger">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/spice/SON/OCCEmotion#Distress"/>
                    <rdf:Description rdf:about="https://w3id.org/spice/SON/OCCEmotion#Reproach"/>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#CompoundEmotion"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
        <owl:disjointWith rdf:resource="https://w3id.org/spice/SON/OCCEmotion#OtherAgent"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#BlameworthyAction -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#BlameworthyAction">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#blameworthy"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
        <owl:disjointWith rdf:resource="https://w3id.org/spice/SON/OCCEmotion#PraiseworthyAction"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#CompoundEmotion -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#CompoundEmotion">
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#Emotion"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#DesirableSituation -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#DesirableSituation">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/spice/SON/OCCEmotion#desirable"/>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
        <owl:disjointWith rdf:resource="https://w3id.org/spice/SON/OCCEmotion#UndesirableSituation"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#Disappointment -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Disappointment">
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevantConfirmed"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#Distress -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Distress">
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectIrrelevant"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectIrrelevant -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectIrrelevant">
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotionSelf"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevant -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevant">
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotionSelf"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevantConfirmed -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevantConfirmed">
        <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevant"/>
    </owl:Class>
    


    <!-- https://w3id.org/spice/SON/OCCEmotion#Emotion -->

    <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Emotion">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
    </owl:Class>
    


