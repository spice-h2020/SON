<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:prov="http://www.w3.org/ns/prov#"
    xmlns:occ="https://w3id.org/spice/SON/OCCEmotion#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:cpannotationschema="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Ontology rdf:about="https://w3id.org/spice/SON/OCCEmotion">
		<owl:versionIRI rdf:resource="https://w3id.org/spice/SON/OCCEmotion/0.0.2"/>
    <owl:imports rdf:resource="https://w3id.org/spice/SON/emotion/0.0.2"/>
    <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl"/>
    <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl"/>
  </owl:Ontology>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Remorse">
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#CompoundEmotion"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Distress"/>
          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Shame"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Hope">
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PositiveOutcomeEmotion"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevant"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Gratification">
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PositiveOutcomeEmotion"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#CompoundEmotion"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Joy"/>
          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Pride"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Reproach">
    <rdfs:comment>The feeling felt by an Appraising Agent for whom some Action / Event, performed by some Other Agent, is blameworthy.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionOther"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
              <owl:Restriction>
                <owl:someValuesFrom>
                  <owl:Class>
                    <owl:intersectionOf rdf:parseType="Collection">
                      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                      <owl:Restriction>
                        <owl:someValuesFrom>
                          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                        </owl:someValuesFrom>
                        <owl:onProperty>
                          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#blameworthy"/>
                        </owl:onProperty>
                      </owl:Restriction>
                      <owl:Restriction>
                        <owl:someValuesFrom>
                          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#OtherAgent"/>
                        </owl:someValuesFrom>
                        <owl:onProperty>
                          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#performedBy"/>
                        </owl:onProperty>
                      </owl:Restriction>
                    </owl:intersectionOf>
                  </owl:Class>
                </owl:someValuesFrom>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#appraises"/>
                </owl:onProperty>
              </owl:Restriction>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#feltBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Joy">
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PositiveOutcomeEmotion"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectIrrelevant"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PraiseworthyAction">
    <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#praiseworthy"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Emotion">
    <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotionOther">
    <rdfs:comment>Emotions classified as being the consequence of some Event happened to Other than Self.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotionSelf"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotion"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion">
    <rdfs:comment>Emotions classified as Negative in respect to the consequence of some Action/Event for the AppraisingAgent.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PolarityOutcomeEmotion"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionSelf">
    <rdfs:comment>Emotions classified as being the consequence of some Action performed by Self.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotion"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
              <owl:Restriction>
                <owl:someValuesFrom>
                  <owl:Class>
                    <owl:intersectionOf rdf:parseType="Collection">
                      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                      <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                          <owl:Restriction>
                            <owl:someValuesFrom>
                              <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                            </owl:someValuesFrom>
                            <owl:onProperty>
                              <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#blameworthy"/>
                            </owl:onProperty>
                          </owl:Restriction>
                          <owl:Restriction>
                            <owl:someValuesFrom>
                              <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                            </owl:someValuesFrom>
                            <owl:onProperty>
                              <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#praiseworthy"/>
                            </owl:onProperty>
                          </owl:Restriction>
                        </owl:unionOf>
                      </owl:Class>
                      <owl:Restriction>
                        <owl:someValuesFrom>
                          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                        </owl:someValuesFrom>
                        <owl:onProperty>
                          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#performedBy"/>
                        </owl:onProperty>
                      </owl:Restriction>
                    </owl:intersectionOf>
                  </owl:Class>
                </owl:someValuesFrom>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#appraises"/>
                </owl:onProperty>
              </owl:Restriction>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#feltBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevantConfirmed">
    <rdfs:comment>Emotions classified as being the consequence of some Event happened to Self in a Scenario in which the Emotion is determined by the State s1 in which the AppraisingAgent is at Time t1 and had been confirmed the previous Emotion felt at Time t. e.g. some AppraisingAgent felt Hope for some desirableEvent at Time t and now he/she feels Satisfaction for the happening of the desirableEvent at Time t1.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevant"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotionSelf">
    <rdfs:comment>Emotions classified as being the consequence of some Event happened to Self.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotion"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#BlameworthyAction">
    <owl:disjointWith rdf:resource="https://w3id.org/spice/SON/OCCEmotion#PraiseworthyAction"/>
    <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#blameworthy"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Pride">
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PositiveOutcomeEmotion"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionSelf"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
              <owl:Restriction>
                <owl:someValuesFrom>
                  <owl:Class>
                    <owl:intersectionOf rdf:parseType="Collection">
                      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                      <owl:Restriction>
                        <owl:someValuesFrom>
                          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                        </owl:someValuesFrom>
                        <owl:onProperty>
                          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#performedBy"/>
                        </owl:onProperty>
                      </owl:Restriction>
                      <owl:Restriction>
                        <owl:someValuesFrom>
                          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                        </owl:someValuesFrom>
                        <owl:onProperty>
                          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#praiseworthy"/>
                        </owl:onProperty>
                      </owl:Restriction>
                    </owl:intersectionOf>
                  </owl:Class>
                </owl:someValuesFrom>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#appraises"/>
                </owl:onProperty>
              </owl:Restriction>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#feltBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Relief">
    <rdfs:comment>The feeling that arises after knowing that some undesirable Event that was feared has not happened.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PositiveOutcomeEmotion"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevantConfirmed"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Fear">
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevant"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Pity">
    <rdfs:comment>Being sad for some undesirable Event happened to someone Other than Self.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotionOther"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotion">
    <rdfs:comment>Emotions classified as being the consequence of some Action.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#Emotion"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
              <owl:Restriction>
                <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#appraises"/>
                </owl:onProperty>
              </owl:Restriction>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#feltBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Shame">
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionSelf"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
              <owl:Restriction>
                <owl:someValuesFrom>
                  <owl:Class>
                    <owl:intersectionOf rdf:parseType="Collection">
                      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                      <owl:Restriction>
                        <owl:someValuesFrom>
                          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                        </owl:someValuesFrom>
                        <owl:onProperty>
                          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#blameworthy"/>
                        </owl:onProperty>
                      </owl:Restriction>
                      <owl:Restriction>
                        <owl:someValuesFrom>
                          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                        </owl:someValuesFrom>
                        <owl:onProperty>
                          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#performedBy"/>
                        </owl:onProperty>
                      </owl:Restriction>
                    </owl:intersectionOf>
                  </owl:Class>
                </owl:someValuesFrom>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#appraises"/>
                </owl:onProperty>
              </owl:Restriction>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#feltBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Gratitude">
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PositiveOutcomeEmotion"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#CompoundEmotion"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Admiration"/>
          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Joy"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevant">
    <rdfs:comment>Emotions classified as being the consequence of some Event happened to Self in a Scenario in which the Emotion is determined by the State s in which the AppraisingAgent is at Time t waiting for some Event. e.g. the AppraisingAgent is waiting for a result of some Action.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotionSelf"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Satisfaction">
    <rdfs:comment>The feeling felt knowing that, after hoping for it, some desirable Event has happened.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PositiveOutcomeEmotion"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevantConfirmed"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Target">
    <rdfs:comment>The Target of some Emotion, this is a very generic and broad concept, it can be an Action, an Event, a Situation etc.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Disappointment">
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevantConfirmed"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionOther">
    <rdfs:comment>Emotions classified as being the consequence of some Action performed by Other than Self.</rdfs:comment>
    <owl:disjointWith rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionSelf"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotion"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
              <owl:Restriction>
                <owl:someValuesFrom>
                  <owl:Class>
                    <owl:intersectionOf rdf:parseType="Collection">
                      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                      <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                          <owl:Restriction>
                            <owl:someValuesFrom>
                              <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                            </owl:someValuesFrom>
                            <owl:onProperty>
                              <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#blameworthy"/>
                            </owl:onProperty>
                          </owl:Restriction>
                          <owl:Restriction>
                            <owl:someValuesFrom>
                              <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                            </owl:someValuesFrom>
                            <owl:onProperty>
                              <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#praiseworthy"/>
                            </owl:onProperty>
                          </owl:Restriction>
                        </owl:unionOf>
                      </owl:Class>
                      <owl:Restriction>
                        <owl:someValuesFrom>
                          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#OtherAgent"/>
                        </owl:someValuesFrom>
                        <owl:onProperty>
                          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#performedBy"/>
                        </owl:onProperty>
                      </owl:Restriction>
                    </owl:intersectionOf>
                  </owl:Class>
                </owl:someValuesFrom>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#appraises"/>
                </owl:onProperty>
              </owl:Restriction>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#feltBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Anger">
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#CompoundEmotion"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Distress"/>
          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Reproach"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#UndesirableSituation">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#undesirable"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PolarityOutcomeEmotion">
    <rdfs:comment>Emotions classified based on their polarity in respect to the Action or Event for the AppraisingAgent: Positive vs Negative.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#Emotion"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Gloating">
    <rdfs:comment>Being happy for some undesirable Event (e.g. a disgrace) happened to someone Other than Self.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PositiveOutcomeEmotion"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotionOther"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectIrrelevant">
    <rdfs:comment>Emotions classified as being the consequence of some Event happened to Self in a Scenario in which the Emotion is not determined by the State s in which the AppraisingAgent is at Time t waiting for some Event. e.g. the AppraisingAgent is not waiting for a result of some Action.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotionSelf"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent">
    <owl:disjointWith>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#OtherAgent"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Fear-Confirmed">
    <rdfs:comment>The feeling that arises after knowing that some Event that was feared has happened.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectRelevantConfirmed"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Value">
    <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SocialObjectAttribute"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#DesirableSituation">
    <owl:disjointWith rdf:resource="https://w3id.org/spice/SON/OCCEmotion#UndesirableSituation"/>
    <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#desirable"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Admiration">
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PositiveOutcomeEmotion"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ActionConsequenceEmotionOther"/>
    <owl:equivalentClass>
      <owl:Restriction>
        <owl:someValuesFrom>
          <owl:Class>
            <owl:intersectionOf rdf:parseType="Collection">
              <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
              <owl:Restriction>
                <owl:someValuesFrom>
                  <owl:Class>
                    <owl:intersectionOf rdf:parseType="Collection">
                      <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
                      <owl:Restriction>
                        <owl:someValuesFrom>
                          <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#OtherAgent"/>
                        </owl:someValuesFrom>
                        <owl:onProperty>
                          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#performedBy"/>
                        </owl:onProperty>
                      </owl:Restriction>
                      <owl:Restriction>
                        <owl:someValuesFrom rdf:resource="https://w3id.org/spice/SON/OCCEmotion#AppraisingAgent"/>
                        <owl:onProperty>
                          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#praiseworthy"/>
                        </owl:onProperty>
                      </owl:Restriction>
                    </owl:intersectionOf>
                  </owl:Class>
                </owl:someValuesFrom>
                <owl:onProperty>
                  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#appraises"/>
                </owl:onProperty>
              </owl:Restriction>
            </owl:intersectionOf>
          </owl:Class>
        </owl:someValuesFrom>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#feltBy"/>
        </owl:onProperty>
      </owl:Restriction>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#PositiveOutcomeEmotion">
    <rdfs:comment>Emotions classified as Positive in respect to the consequence of some Action/Event for the AppraisingAgent.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#PolarityOutcomeEmotion"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#CompoundEmotion">
    <rdfs:comment>Emotions being the combinations of two other Emotions.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#Emotion"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Distress">
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#ECES_ProspectIrrelevant"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#Resentment">
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#NegativeOutcomeEmotion"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotionOther"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#OtherAgent">
    <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#HappyFor">
    <rdfs:comment>Being happy for some desirable Event happened to someone Other than Self.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#PositiveOutcomeEmotion"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotionOther"/>
  </owl:Class>
  <owl:Class rdf:about="https://w3id.org/spice/SON/OCCEmotion#EventConsequenceEmotion">
    <rdfs:comment>Emotions classified as being the consequence of some Event.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/spice/SON/OCCEmotion#Emotion"/>
  </owl:Class>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#lovedBy">
    <rdfs:comment>Some Agent is lovedBy some OtherAgent, this could explain the e.g. feeling of Joy of Agent1 for some desirable Event happend to some Agent2.</rdfs:comment>
    <owl:propertyDisjointWith>
      <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#notLovedBy"/>
    </owl:propertyDisjointWith>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#loves"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#desirable">
    <rdfs:comment>Some Situation is desirable for some Agent.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#feels">
    <rdfs:comment>Some Agent feels some Emotion.</rdfs:comment>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#feltBy"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#hasAgent">
    <rdfs:comment>Some Action or Event or Process hasAgent some Agent.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#notLovedBy">
    <rdfs:comment>Some Agent is notLovedBy some OtherAgent, this could explain the e.g. feeling of Joy of Agent1 for some undesirable Event happend to some Agent2.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#notLoves"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#loves">
    <rdfs:comment>Some Agent loves some OtherAgent, this could imply a transitivity of the Emotion feeling.
e.g. Agent2 feels EmotionX ^ Agent1 loves Agent2 â†’ Agent1 feels EmotionX .
Or could determine some internal conflict, depending on how much complex is the scenario.</rdfs:comment>
    <owl:propertyDisjointWith>
      <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#notLoves"/>
    </owl:propertyDisjointWith>
    <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#appraises">
    <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#blameworthy">
    <rdfs:comment>Some Entity is blameworthy for some AppraisingAgent.</rdfs:comment>
    <owl:propertyDisjointWith>
      <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#praiseworthy"/>
    </owl:propertyDisjointWith>
    <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#hasValue">
    <rdfs:comment>Some Agent hasValue some Value.</rdfs:comment>
    <rdfs:range rdf:resource="https://w3id.org/spice/SON/OCCEmotion#Value"/>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#praiseworthy">
    <rdfs:comment>Some Entity is praiseworthy for some AppraisingAgent.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#performedBy">
    <rdfs:comment>Some Process is performedBy some Agent.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#notLoves">
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#undesirable">
    <rdfs:comment>Some Situation is undesirable for some Agent.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#hasGoal">
    <rdfs:comment>Some Agent hasGoal some Goal.</rdfs:comment>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#target"/>
  <owl:ObjectProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#feltBy">
    <rdfs:comment>Some Emotion is feltBy some Agent.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#confirmed">
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf>
          <rdf:List>
            <rdf:rest>
              <rdf:List>
                <rdf:rest>
                  <rdf:List>
                    <rdf:rest>
                      <rdf:List>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                        <rdf:first>unconfirmed</rdf:first>
                      </rdf:List>
                    </rdf:rest>
                    <rdf:first>irrelevant</rdf:first>
                  </rdf:List>
                </rdf:rest>
                <rdf:first>disconfirmed</rdf:first>
              </rdf:List>
            </rdf:rest>
            <rdf:first>confirmed</rdf:first>
          </rdf:List>
        </owl:oneOf>
      </rdfs:Datatype>
    </rdfs:range>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="https://w3id.org/spice/SON/OCCEmotion#accomplished">
    <rdfs:comment>Aspectual description of the Situation.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
  </owl:DatatypeProperty>
</rdf:RDF>
